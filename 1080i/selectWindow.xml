<window id="5535">
	<defaultcontrol always="true">2000</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="group">
			<left>600</left>
			<top>460</top>
			<animation type="WindowOpen" reversible="false">
				<effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
				<effect type="fade" start="0" end="100" time="300" />
			</animation>
			<animation type="WindowClose" reversible="false">
				<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
				<effect type="fade" start="100" end="0" time="300" />
			</animation>
			<control type="image">
				<left>5</left>
				<top>5</top>
				<width>600</width>
				<height>174</height>
				<texture colordiffuse="PrimaryBG">special://skin/extras/backgrounds/white.png</texture>
			</control>
			<control type="image">
				<left>5</left>
				<top>5</top>
				<width>600</width>
				<height>174</height>
				<texture colordiffuse="PrimaryBG">special://skin/extras/backgrounds/white.png</texture>
			</control>
			<control type="group">
				<left>17</left>
				<top>16</top>
				<width>590</width>
				<height>150</height>
				<control type="list" id="2000">
					<left>0</left>
					<top>0</top>
					<width>590</width>
					<height>150</height>
					<onright>121</onright>
					<pagecontrol>121</pagecontrol>
					<scrolltime>300</scrolltime>
					<visible>true</visible>
					<itemlayout height="30">
						<control type="image">
							<left>2</left>
							<top>2</top>
							<width>555</width>
							<height>28</height>
							<texture border="90,3,90,3">views/list/other/separator.png</texture>
						</control>
						<control type="label">
							<left>20</left>
							<top>4</top>
							<width>190</width>
							<height>26</height>
							<textcolor>SecondaryT</textcolor>
							<font>Font-Condensed-S30-B</font>
							<label>$INFO[ListItem.Label]</label>
						</control>
						<control type="label">
							<left>140</left>
							<top>4</top>
							<width>400</width>
							<height>26</height>
							<textcolor>SecondaryT</textcolor>
							<font>Font-Condensed-S30-B</font>
							<align>right</align>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</itemlayout>
					<focusedlayout height="30">
						<control type="image">
							<left>2</left>
							<top>2</top>
							<width>555</width>
							<height>28</height>
							<texture colordiffuse="TertiaryM">special://skin/extras/backgrounds/white.png</texture>
						</control>
						<control type="image"> <!--focused background left edge -->
							<posx>0</posx>
							<posy>0</posy>
							<width>5</width>
							<height>28</height>
							<texture condition="Control.HasFocus(30)" colordiffuse="$VAR[FocusModHomeColourVar]">special://skin/extras/backgrounds/white.png</texture>
						</control>
						<control type="label">
							<left>20</left>
							<top>4</top>
							<width>190</width>
							<height>26</height>
							<textcolor>$VAR[FocusModColourVar]</textcolor>
							<font>Font-Condensed-S30-B</font>
							<label>$INFO[ListItem.Label]</label>
						</control>
						<control type="label">
							<left>140</left>
							<top>4</top>
							<width>400</width>
							<height>26</height>
							<textcolor>$VAR[FocusModColourVar]</textcolor>
							<font>Font-Condensed-S30-B</font>
							<align>right</align>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</focusedlayout>
				</control>
			</control>
			<control type="scrollbar" id="121">
				<description>scroll bar indicator for lists</description>
				<left>575</left>
				<top>10</top>
				<width>24</width>
				<height>165</height>
				<onleft>2000</onleft>
				<texturesliderbackground>-</texturesliderbackground>
				<texturesliderbar colordiffuse="PrimaryT" border="1">views/other/scrollbar-sq.png</texturesliderbar>
				<texturesliderbarfocus colordiffuse="PrimaryT" border="1">views/other/scrollbar-sq.png</texturesliderbarfocus>
				<textureslidernib>-</textureslidernib>
				<textureslidernibfocus>-</textureslidernibfocus>
				<orientation>vertical</orientation>
				<showonepage>false</showonepage>
			</control>
		</control>
	</controls>
</window>